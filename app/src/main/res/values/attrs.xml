<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeLine">
        <attr name="showCounts" format="boolean" />
        <attr name="labelPosition" format="enum">
            <enum name="bottom" value="0"/>
            <enum name="top" value="1"/>
        </attr>
        <attr name="doneColor" format="color"/>
        <attr name="lateColor" format="color"/>
        <attr name="skipColor" format="color"/>
        <attr name="noneColor" format="color"/>

        <attr name="textColor" format="color"/>
    </declare-styleable>
</resources>